{
  "name": "MyProject",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "ts-node": "3.3.0",
    "@types/node": "^8.0.29",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "mysql": "^2.14.1",
    "reflect-metadata": "^0.1.13",
    "tsc-watch": "^4.0.0",
    "typeorm": "^0.2.20"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/src/app.js\"",
    "dev": "ts-node src/app.ts",
    "01": "ts-node src/ts/01connect",
    "02": "ts-node src/ts/02table",
    "03": "ts-node src/ts/03crud",
    "04": "ts-node src/ts/04one2one",
    "05": "ts-node src/ts/05one2m",
    "06": "ts-node src/ts/06m2n",
    "docker": "docker-compose up --build",
    "stop": "docker-compose down",
    "mg:gn": "typeorm migration:generate -n ALL",
    "mg:cr": "typeorm migration:create -n GroupMembdsafdsafer",
    "mg:up": "typeorm migration:run",
    "mg:dw": "typeorm migration:revert"
  }
}
