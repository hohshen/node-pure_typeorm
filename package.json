{
   "name": "MyProject",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "ts-node": "3.3.0",
      "@types/node": "^8.0.29",
      "typescript": "3.3.3333"
   },
   "dependencies": {
      "mysql": "^2.14.1",
      "reflect-metadata": "^0.1.10",
      "tsc-watch": "^4.0.0",
      "typeorm": "0.2.20"
   },
   "scripts": {
      "build": "rimraf dist && tsc -p tsconfig.build.json",
      "start": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/index.js\"",
      "01": "ts-node src/01connect",
      "02": "ts-node src/02table",
      "03": "ts-node src/03crud",
      "04": "ts-node src/04one2one",
      "05": "ts-node src/05one2m",
      "06": "ts-node src/06m2n",
      "07": "ts-node src/07tree",
      "docker": "docker-compose -p MyProject up  --build",
      "stop": "docker-compose -p MyProject  down",
      "mg:gn": "typeorm migration:generate -n ALL",
      "mg:cr": "typeorm migration:create -n GroupMember",
      "mg:up": "typeorm migration:run",
      "mg:dw": "typeorm migration:revert"
   }
}
