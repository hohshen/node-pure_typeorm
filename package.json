{
  "name": "MyProject",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "ts-node": "3.3.0",
    "@types/node": "^8.0.29",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "mysql": "^2.14.1",
    "reflect-metadata": "^0.1.13",
    "tsc-watch": "^4.0.0",
    "typeorm": "^0.2.20"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "start": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/src/index.js\"",
    "dev": "ts-node src/index.ts",
    "docker": "docker-compose up --build",
    "stop": "docker-compose down",
    "mg:gn": "typeorm migration:generate -n ALL",
    "mg:cr": "typeorm migration:create -n GroupMembdsafdsafer",
    "mg:up": "typeorm migration:run",
    "mg:dw": "typeorm migration:revert"
  }
}
